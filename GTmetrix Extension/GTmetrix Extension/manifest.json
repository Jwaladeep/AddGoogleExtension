{
"name": "Google Extension for Bookmarks",
"version": "1.1",
"description": "If a browser action to detect love between two birds, Jwaladeep and Pratiyush",
"permissions": [
"bookmarks"
],
"browser_action": {
"default_title": "My Bookmarks",
"default_icon": "icon.png",
"default_popup": "popup.html"
},
"manifest_version": 2,
"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}